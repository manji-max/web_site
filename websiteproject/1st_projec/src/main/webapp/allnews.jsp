    }

    .navbar a:hover {
        text-decoration: underline;
    }

    /* Section header */
    .section-header {
        font-size: 1.5em;
        font-weight: bold;
        margin: 20px 0;
        border-bottom: 2px solid #ddd;
        padding-bottom: 10px;
